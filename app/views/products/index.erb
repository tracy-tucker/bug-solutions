<h1>All Products</h1>

<div>
    <% @product.each do |product| %>
        <% if product.user == current_user %>
            <p><a href="/products/<%= product.id %>"><%= product.product_name %></a>
        <% else %>
            <p><%=product.product_name%> - added by <%= product.user.username %></p>
        <%end%>
    <%end%>
</div>
